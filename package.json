    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "device-detector-js": "^2.2.1",
    "eslint": "^7.0.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "faker": "^4.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "moment": "^2.25.3",
    "moment-timezone": "^0.5.29",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
